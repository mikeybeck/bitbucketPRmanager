{
  "name": "Bitbucket PR Manager",
  "version": "0.1",
  "description": "Manage your Bitbucket pull requests",
  "permissions": ["activeTab", "declarativeContent", "storage", "alarms"],
  "options_page": "options.html",
  "background": {
    "scripts": ["jquery-3.4.1.min.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
